<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/b_camera"
        android:layout_width="match_parent"
        android:layout_height="43dp"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="3dp"
        android:backgroundTint="@color/teal_200"
        android:text="Camera"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/b_contact"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b_call" />

    <Button
        android:id="@+id/b_call"
        android:layout_width="match_parent"
        android:layout_height="43dp"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="3dp"
        android:backgroundTint="@color/teal_200"
        android:text="Call"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/b_camera"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText123" />

    <Button
        android:id="@+id/b_dialpad"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="3dp"
        android:backgroundTint="@color/teal_200"
        android:text="Dialpad"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/b_assignment"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b_gallery" />

    <Button
        android:id="@+id/b_assignment"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="3dp"
        android:backgroundTint="@color/teal_200"
        android:text="Assignment"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b_dialpad" />

    <Button
        android:id="@+id/b_gallery"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="3dp"
        android:backgroundTint="@color/teal_200"
        android:text="Gallery"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/b_dialpad"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b_browser" />

    <Button
        android:id="@+id/b_browser"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="3dp"
        android:backgroundTint="@color/teal_200"
        android:text="Browser"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/b_gallery"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b_contact" />

    <Button
        android:id="@+id/b_contact"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="3dp"
        android:backgroundTint="@color/teal_200"
        android:text="Contact"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/b_browser"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b_camera" />

    <EditText
        android:id="@+id/editText123"
        android:layout_width="0dp"
        android:layout_height="46dp"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="3dp"
        android:ems="10"
        android:hint="Enter Text"
        android:inputType="textPersonName"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/b_call"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>